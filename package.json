{
  "name": "simprov",
  "version": "0.0.1",
  "description": "Shareable Interactive Manipulation Provenance",
  "main": "sourceScripts/index.js",
  "scripts": {
    "prebuild": "npm-run-all cleanDist",
    "build": "babel-node buildScripts/build.js",
    "postbuild": "babel-node buildScripts/distServer.js",
    "cleanDist": "rimraf ./dist && mkdir dist",
    "copyAssests": "copyfiles -u 1 public/css/*.css dist && copyfiles -u 1 public/js/*.js dist",
    "deploy": "surge ./dist",
    "devServer": "babel-node buildScripts/devServer.js",
    "integrity": "nsp check",
    "lint": "esw sourceScripts buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "localtunnel": "lt --port 3000",
    "share": "npm-run-all --parallel devServer localtunnel",
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel integrity",
    "test": "mocha --reporter mochawesome --reporter-options reportDir=testReports,reportFilename=report,reportTitle=Report,reportPageTitle=Report,quiet=true buildScripts/testSetup.js \"sourceScripts/**/*.test.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akhi4u/Research.git"
  },
  "keywords": [
    "Shareable",
    "Interactive",
    "Manipulation",
    "Provenance"
  ],
  "author": "akhi4u",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/akhi4u/Research/issues"
  },
  "homepage": "https://github.com/akhi4u/Research#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-env": "^1.3.3",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "copyfiles": "^1.2.0",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-watch": "^3.0.1",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "localtunnel": "^1.8.2",
    "mocha": "^3.2.0",
    "mochawesome": "^2.0.4",
    "npm-run-all": "^4.0.2",
    "nsp": "^2.6.3",
    "open": "^0.0.5",
    "read-pkg": "^2.0.0",
    "surge": "^0.19.0",
    "uglify-js": "^2.8.22",
    "uglifyjs-webpack-plugin": "^0.4.1",
    "webpack": "^2.3.3",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-md5-hash": "^0.0.5"
  },
  "dependencies": {
    "cuid": "^1.3.8",
    "dexie": "^1.5.1",
    "es6-mixin": "^0.3.0",
    "rasterizehtml": "^1.2.4",
    "tree-model": "^1.0.6"
  }
}
